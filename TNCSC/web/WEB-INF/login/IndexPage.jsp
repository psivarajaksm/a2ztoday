<%@ taglib uri="/WEB-INF/struts-html.tld" prefix="html"%><%@ taglib uri="/WEB-INF/struts-bean.tld" prefix="bean"%><%@ taglib uri="/WEB-INF/struts-logic.tld" prefix="logic"%><%@ page contentType="text/html"%><%@ page pageEncoding="UTF-8"%><%@ page import="com.onward.common.ApplicationConstants"%><%    String productHTML = "";    String staticPath = AppProps.getInstance().getProperty(ApplicationConstants.STATIC_IP_PATH);    UserViewModel userObj = (UserViewModel) request.getSession(false).getAttribute("userDetails");    String userId = userObj.getUserid();    if ((request.getAttribute("productHTML") != null)) {        productHTML = (String) request.getAttribute("productHTML");    }%><!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"    "http://www.w3.org/TR/html4/loose.dtd"><html>    <head>           <link type="text/css" href="<%=staticPath%>css/ui-lightness/jquery-ui-1.8.21.custom.css" rel="stylesheet" />        <script type="text/javascript" src="<%=staticPath%>scripts/jquery-1.7.2.min.js"></script>        <script type="text/javascript" src="<%=staticPath%>scripts/jquery-ui-1.8.21.custom.min.js"></script>        <script type="text/javascript" src="<%=staticPath%>scripts/jquery.tablesorter.js"></script>         <script type="text/javascript" src="<%=staticPath%>scripts/jquery.tablesorter.pager.js"></script>         <script src="dwr/interface/LoginAction.js"></script>        <script src="dwr/interface/UserAction.js"></script>        <script src="dwr/interface/UserTypeAction.js"></script>        <script src="dwr/engine.js"></script>        <script src="dwr/util.js"></script>        <script>            $(function() {                $('#financialyearform').dialog({                    autoOpen: false,                    width: 600,                    modal: true,                    buttons: {                        "Select": function() {                            var financialyear=document.getElementById('financialyear').value;                            if(financialyear=="0"){                                alert("Please Select the Financial Year")                                document.getElementById('financialyear').focus();                                return false;                            }else{                                document.getElementById('fyearvalue').value=financialyear;                                $(this).dialog("close");                                getBlanket('continueDIV');                                UserTypeAction.FinacialYearStatus(financialyear,finacialyearsetSession);                                                                //                                document.forms[0].action="UserAction.htm";                                //                                document.forms[0].method.value="setFinacialYear";                                //                                document.forms[0].submit();                                //                                $(this).dialog("close");                            }                        }                    }                });            });            function setProductType(obj){                document.getElementById('currentusermodule').value = obj.id;                document.getElementById('multiaccountmodule').value = obj.value;                //                document.forms[0].currentusermodule.value = obj.id;                if(obj.value=="Yes"){                    $('#financialyearform').dialog('open');                }else{                    getBlanket('continueDIV');                    document.forms[0].action="UserAction.htm";                    document.forms[0].method.value="setProductType";                    document.forms[0].submit();                }            }            function finacialyearsetSession(map){                if(map.success!=null && map.success!=""){                    getBlanket('continueDIV');                    var multiaccountmodule=document.getElementById('multiaccountmodule').value;                    if(multiaccountmodule=="Yes"){                        getBlanket('continueDIV');                        document.forms[0].action="UserAction.htm";                        document.forms[0].method.value="setProductType";                        document.forms[0].submit();                    }                }            }        </script>        </head>    <body topmargin="0" marginheight="0" leftmargin="0" marginleft="0" valign="center">        <div id="blanket" style="display:none;"></div>        <div id="continueDIV" style="display:none;" align="center" class="loadingRequest">            <img src="<%=staticPath%>images/loading_1.gif" align="middle" border="0"><br /><bean:message key="reqinprogress" />        </div>        <form method="post">            <div id="header">                <%@ include file="/common/header.jsp" %>            </div>            <br>            <div>                              <marquee behavior="slide" direction="left"><h1>Kind Attn: DA(113) has been updated/changed. New DA 119% will be applied in pay bill</h1></marquee>                <div id="content">                    <div id="tabnew">                        <br><br>                        <div id="moduledetails" style="height:550px;overflow:auto;" align="center">                            <table id="commonBodyTable" border="0" cellspacing="0" cellpadding="0" align="center">                                <tr>                                    <td width="100%" align="center">                                        <table width="100%" align="center" border="0" cellspacing="0" cellpadding="0" >                                            <tr>                                                <td>                                                    <div id="moduleHTML"></div>                                                </td>                                            </tr>                                        </table>                                    </td>                                </tr>                            </table>                        </div>                    </div>                </div>            </div>            <div id="financialyearform" title="Financial Year Form" >                <table table width="100%" align="center" border="0" cellpadding="0" cellspacing="0">                    <tr class="lightrow">                        <td width="20%" class="textalign">Financial Year</td>                        <td width="5%" class="mandatory">*</td>                        <td width="25%"class="textfieldalign"><select class="combobox" name="financialyear" id="financialyear"></select></td>                    </tr>                </table>            </div>            <div id="footer">                <%@ include file="/common/footer.jsp" %>            </div>            <input type="hidden" name="method">            <input type="hidden" name="currentusermodule" id="currentusermodule">            <input type="hidden" name="fyearvalue" id="fyearvalue" value="0">            <input type="hidden" name="multiaccountmodule" id="multiaccountmodule" value="No">        </form>    </body>        <script type="text/javascript">        onbodyload();        function onbodyload(){            UserAction.getUserTypeSize(fillHomeIndexMenu);        }        function fillHomeIndexMenu(map){            if(map.typesize==0 || map.typesize==1){                if(map.isAccountModule=="Yes"){                    $('#financialyearform').dialog('open');                }                document.getElementById('tabnew').style.display="none";            }else{                document.getElementById("moduleHTML").innerHTML='<%=productHTML%>';                        }            dwr.util.removeAllOptions("financialyear");            dwr.util.addOptions("financialyear",map.financialYearMap);        }    </script></html>